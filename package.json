    "express-ejs-layouts": "^2.5.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "laravel-mix": "^6.0.49",
    "nodemon": "^3.0.1",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.67.0",
